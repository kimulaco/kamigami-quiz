{
  "name": "kamigami-quiz",
  "version": "1.0.0",
  "description": "「神の一覧 - Wikipedia」の情報を元に作成したジョーククイズサイト。",
  "author": "kimulaco",
  "private": true,
  "scripts": {
    "dev": "npm run update && nuxt",
    "build": "npm run update && nuxt build",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "start": "nuxt start",
    "generate": "npm run update && nuxt generate",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "update": "node bin/updata.js"
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "^2.2.0",
    "cross-env": "^5.2.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "^0.1.2",
    "babel-eslint": "^8.2.1",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.11.0",
    "request": "^2.88.0",
    "sass-loader": "^7.1.0"
  }
}
